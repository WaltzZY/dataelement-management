    <select id="getProcessRecoderForPublished" resultMap="BaseResultMap">
        SELECT *
        FROM process_record pr
        WHERE pr.base_dataelement_dataid = #{dataid}
          AND pr.destactivityname = 'PUBLISHED'
        LIMIT 1
    </select>

    <select id="getRevisionRecordByDataId" resultMap="BaseResultMap">
        SELECT *
        FROM process_record pr
        WHERE pr.base_dataelement_dataid = #{dataid}
          AND pr.useroperation = '驳回'
        ORDER BY pr.processdatetime DESC
        LIMIT 1
    </select>

    <select id="getAllRevisionRecordsByDataId" resultMap="BaseResultMap">
        SELECT *
        FROM process_record pr
        WHERE pr.base_dataelement_dataid = #{dataid}
          AND pr.useroperation = '驳回'
        ORDER BY pr.processdatetime DESC
    </select>

    <select id="getProcessRecordsByDataId" resultMap="BaseResultMap">
        SELECT *
        FROM process_record pr
        WHERE pr.base_dataelement_dataid = #{dataid}
        ORDER BY pr.processdatetime ASC
    </select>

</mapper>