    <result column="base_dataelement_dataid" jdbcType="VARCHAR" property="baseDataelementDataid" />
    <result column="flowid" jdbcType="VARCHAR" property="flowid" />
    <result column="processuserid" jdbcType="VARCHAR" property="processuserid" />
    <result column="processusername" jdbcType="VARCHAR" property="processusername" />
    <result column="processunitcode" jdbcType="VARCHAR" property="processunitcode" />
    <result column="processunitname" jdbcType="VARCHAR" property="processunitname" />
    <result column="useroperation" jdbcType="VARCHAR" property="useroperation" />
    <result column="processdatetime" jdbcType="TIMESTAMP" property="processdatetime" />
    <result column="sourceactivityname" jdbcType="VARCHAR" property="sourceactivityname" />
    <result column="destactivityname" jdbcType="VARCHAR" property="destactivityname" />
    <result column="create_date" jdbcType="TIMESTAMP" property="createDate" />
    <result column="create_account" jdbcType="VARCHAR" property="createAccount" />
